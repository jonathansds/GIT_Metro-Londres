package com.teste.londres.algoritmo;

import java.util.List;

/**
 * @author Jonathan
 * @version 13/05/2014
 *
 */
public class Estacao {


	private String nome;
	
	
	/**
	 * @return the caminhosPossiveis
	 */
	public List<Caminho> getCaminhosPossiveis() {
		return caminhosPossiveis;
	}

	/**
	 * @param caminhosPossiveis the caminhosPossiveis to set
	 */
	public void setCaminhosPossiveis(List<Caminho> caminhosPossiveis) {
		this.caminhosPossiveis = caminhosPossiveis;
	}

	/**
	 * @return the nome
	 */
	public String getNome() {
		return nome;
	}

	/**
	 * @param nome the nome to set
	 */
	public void setNome(String nome) {
		this.nome = nome;
	}
}
